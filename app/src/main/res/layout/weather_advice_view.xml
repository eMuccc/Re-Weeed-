<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="120dp"
    android:layout_height="150dp"
    android:orientation="vertical">



        <!--            style="?android:attr/progressBarStyleHorizontal"-->


    <TextView
        android:id="@+id/tv_type"
        android:layout_width="match_parent"
        android:layout_height="24dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/zs"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />


    <com.liys.view.WaterWaveProView
        android:id="@+id/speed360"
        android:layout_width="76dp"
        android:layout_height="79dp"
        android:layout_gravity="center"
        app:light_color="@color/light"
        app:light_show="true"
        app:progress_color="@color/green"
        app:progress_color_background="@color/light_green"
        app:progress_max="10"
        app:progress_value="5"
        app:water_wave_speed="4"/>
    
    <TextView
        android:id="@+id/tv_category"
        android:layout_width="match_parent"
        android:layout_height="24dp"

        android:layout_marginBottom="15dp"
        android:gravity="center"
        android:text="@string/zszt"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintVertical_bias="0.0" />

    
</LinearLayout>